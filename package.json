{
  "name": "@serh/vue-roundslider",
  "version": "1.0.0",
  "description": "Vue component round slider (can call as Circle slider or Knob slider) component allows the user to select a value or range of values through a circular dial",
  "main": "dist/vue-roundslider.umd.js",
  "module": "dist/vue-roundslider.esm.js",
  "unpkg": "dist/vue-roundslider.min.js",
  "browser": {
    "./sfc": "src/vue-roundslider.vue"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-roundslider.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-roundslider.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-roundslider.min.js"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serhn/vue-roundslider.git"
  },
  "keywords": [
    "vue",
    "vue-compoment",
    "slider",
    "round",
    "360"
  ],
  "author": "Serhii Nikolaichuk",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/serhn/vue-roundslider/issues"
  },
  "homepage": "https://github.com/serhn/vue-roundslider#readme"
}
